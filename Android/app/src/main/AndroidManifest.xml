<?xml version="1.0" encoding="utf-8"?>
<!--suppress ALL -->
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:fullBackupContent="@xml/backup_descriptor">
        <activity
            android:name=".Views$Travel_Launch"
            android:label="@string/title_activity_travel__launch"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Views$PlayerInventoryLaunch"
            android:label="@string/title_activity_player_inventory_launch"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Views$Marketplace_Sell_Startup"
            android:label="@string/title_activity_marketplace__sell__startup"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Views$Marketplace_PlayerInventory_Startup"
            android:label="@string/title_activity_marketplace__player_inventory__startup"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Views$MarketplaceStartup"
            android:label="@string/title_activity_marketplace_startup"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Views$ConfigureCompleteActivity"
            android:label="@string/title_activity_configure_complete"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Views$StartUpActivity"
            android:label="@string/title_activity_start_up"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Views$MainActivity"
            android:exported="true"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>